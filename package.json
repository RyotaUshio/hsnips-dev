{
  "name": "hsnips-dev",
  "version": "0.1.0",
  "description": "My setup for developing HyperSnips snippets.",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && snipbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "test": "vitest run"
  },
  "author": "Ryota Ushio",
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "snipbuild": "^0.2.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.8"
  }
}
